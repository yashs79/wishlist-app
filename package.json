{
  "name": "flockshop",
  "version": "1.0.0",
  "description": "A real-time collaborative wishlist application",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "wishlist",
    "collaboration",
    "real-time",
    "mern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "concurrently": "^8.2.0"
  }
}
